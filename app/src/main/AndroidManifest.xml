<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spiderinductions.rb.spidertask2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".textdisplayactivity"
            android:label="@string/title_activity_textdisplayactivity"
            android:parentActivityName=".MainActivity" >
        </activity>
        <activity
            android:name=".fontfacelistactivity"
            android:label="@string/title_activity_fontfacelistactivity">
        </activity>
        <activity
            android:name=".textdisplayactivity2"
            android:label="@string/title_activity_textdisplayactivity2">
        </activity>
    </application>

</manifest>
